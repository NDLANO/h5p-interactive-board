{
  "name": "h5p-course-presentation-alpha",
  "version": "1.20.0",
  "description": "A free HTML5- based presentation content type which allows users to add multiple choice, fill in the blanks, texts and other types of interactions to their presentations using only a web browser.",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV='production' webpack --mode=production",
    "watch": "webpack --watch --mode=development",
    "validate-types": "tsc --noEmit",
    "format:fix": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h5p/h5p-course-presentation.git"
  },
  "keywords": [
    "h5p",
    "content-type"
  ],
  "author": "Joubel",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
    "@babel/plugin-transform-object-assign": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
    "babel-loader": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "h5p-lib-controls": "0.0.15",
    "h5p-parent": "0.0.2",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.56.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "focus-trap": "^7.0.0"
  }
}
